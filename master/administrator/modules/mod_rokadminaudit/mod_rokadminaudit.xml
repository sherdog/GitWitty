<?xml version="1.0" encoding="utf-8"?>
<install type="module" version="1.5.0" method="upgrade" client="administrator">
	<name>RokAdminAudit</name>
	<creationDate>December 6, 2010</creationDate>
	<author>RocketTheme, LLC</author>
	<authorEmail>support@rockettheme.com</authorEmail>
	<authorUrl>http://www.rockettheme.com</authorUrl>
	<copyright>(C) 2005 - 2010 RocketTheme, LLC. All rights reserved.</copyright>
	<license>http://www.gnu.org/copyleft/gpl.html GNU/GPL</license>
	<version>0.1.3</version>
	<description>This module provides admin audit info gathered by the RokTracking plugin</description>
	<files>
		<filename module="mod_rokadminaudit">mod_rokadminaudit.php</filename>
		<filename>helper.php</filename>
		<filename>index.html</filename>
		<folder>tmpl/</folder>
	</files>
	<params>
		<param name="limit" type="text" default="5" label="Entries Limit" description="" />
		<param name="purgedays" type="text" default="30" label="Entry History" description="" />
		<param name="detail_filter" type="list" default="low" label="Detail Filter" description="">
			<option value="low">Low Detail</option>
			<option value="medium">Medium Detail</option>
			<option value="high">High Detail</option>
			</param>
	</params>
</install>
