<?xml version="1.0" encoding="utf-8"?>
<install type="module" version="1.5.0" method="upgrade" client="administrator">
	<name>RokUserChart</name>
	<creationDate>December 6, 2010</creationDate>
	<author>RocketTheme, LLC</author>
	<authorEmail>support@rockettheme.com</authorEmail>
	<authorUrl>http://www.rockettheme.com</authorUrl>
	<copyright>(C) 2005 - 2010 RocketTheme, LLC. All rights reserved.</copyright>
	<license>http://www.gnu.org/copyleft/gpl.html GNU/GPL</license>
	<version>0.1.3</version>
	<description>This module provides a chart of user activity gathered by the RokTracking plugin</description>
	<files>
		<filename module="mod_rokuserchart">mod_rokuserchart.php</filename>
		<filename>helper.php</filename>
		<filename>index.html</filename>
		<folder>tmpl/</folder>
		<folder>googlechartlib/</folder>
	</files>
	<params>
		<param name="extra_class" type="text" default="" label="Extra Class" description="" />
		<param name="history" type="text" default="7" label="Days of History" description="" />
	</params>
</install>
